//arithmetic operators

#include<stdio.h>
int main()
{
    int a=30,b=20;
    printf("a + b = %d\n",a+b);
    printf("a - b = %d\n",a-b);
    printf("a * b = %d\n",a*b);
    printf("a / b = %d\n",a/b);
    printf("a % b = %d\n",a%b);
    printf("+a = %d\n",+a);
    printf("-a = %d\n",-a);
    printf("a++ = %d\n",a++);
    printf("a-- = %d\n",a--);
    
    return 0;
}

//relational operators
#include<stdio.h>
int main()
{
    int a,b;
    printf("Enter the number 1 and number 2 :\n");
    scanf("%d %d",&a,&b);
    printf("a < b = %d\n",a<b);
    printf("a > b = %d\n",a>b);
    printf("a <= b = %d\n",a<=b);
    printf("a >= b = %d\n",a/b);
    printf("a == b = %d\n",a==b);
    printf("a != b = %d\n",a!=b);
    
    
    return 0;
}
