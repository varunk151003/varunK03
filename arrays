//2 dimensional array with numbers

#include<stdio.h>
int main()
{
    int arr[2][3];
    printf("2D array:\n");
    
    for(int i=0;i<2;i++){
        for(int j=0;j<3;j++){
            printf("\nEnter the number or character :");
            scanf("%d",&arr[i][j]);
        }
        
        printf("\n");
    }
    
    printf("The 2D array is:\n");
    for(int i=0;i<2;i++){
        for(int j=0;j<3;j++){
            printf("%d ",arr[i][j]);
        }
        printf("\n");
    }
    return 0;
}
